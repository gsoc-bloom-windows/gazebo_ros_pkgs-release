<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-gazebo-plugins</id>
    <version>3.3.1</version>
    <title>ros-dashing-gazebo-plugins</title>
    <authors>Jose Luis Rivero</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Robot-independent Gazebo plugins for sensors, motors and dynamic reconfigurable components.</description>
    <dependencies>
		<dependency id="ros-dashing-camera-info-manager"/>
		<dependency id="ros-dashing-cv-bridge"/>
		<dependency id="ros-dashing-gazebo-dev"/>
		<dependency id="ros-dashing-gazebo-ros"/>
		<dependency id="ros-dashing-geometry-msgs"/>
		<dependency id="ros-dashing-image-transport"/>
		<dependency id="ros-dashing-nav-msgs"/>
		<dependency id="ros-dashing-rclcpp"/>
		<dependency id="ros-dashing-sensor-msgs"/>
		<dependency id="ros-dashing-std-msgs"/>
		<dependency id="ros-dashing-tf2-geometry-msgs"/>
		<dependency id="ros-dashing-tf2-ros"/>
		<dependency id="ros-dashing-ros-workspace"/>

    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
